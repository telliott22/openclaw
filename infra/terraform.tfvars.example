# Hetzner Cloud API token
# Create one at: https://console.hetzner.cloud → Project → Security → API Tokens
hcloud_token = "your-hetzner-api-token"

# OpenClaw secrets — generate with: openssl rand -hex 32
openclaw_gateway_token = "generate-me"
gog_keyring_password   = "generate-me"

# Optional overrides (defaults are fine)
# server_type         = "cx23"
# location            = "nbg1"
# ssh_public_key_path = "~/.ssh/id_ed25519.pub"
